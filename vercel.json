{
	"rewrites": [
		{
			"source": "/",
			"destination": "https://netlify-site-example.netlify.app/"
		},
		{
			"source": "*",
			"destination": "https://netlify-site-example.netlify.app/"
		},
		{
			"source": "(*)",
			"destination": "https://netlify-site-example.netlify.app/"
		},
		{
			"source": "(.*)",
			"destination": "https://netlify-site-example.netlify.app/"
		}
	]
}
